{
  "connections": [ { "port": 8800 } ],
  "registrations": [
    { "plugin": "vision" },
    {
      "plugin": {
        "register": "bedwetter",
        "options": {
          "prefix": "/woot"
        }
      }
    },
    {
      "plugin": {
        "register": "visionary",
        "options": {
          "engines": { "pug": "pug" },
          "path": "tpls"
        }
      }
    },
    {
      "plugin": {
        "register": "dogwater",
        "options": {
          "connections": {
            "memoryDB": {
              "adapter": "memory"
            }
          },
          "adapters": {
            "memory": "sails-memory"
          },
          "models": "lib/models",
          "fixtures": [{
            "model": "dogs",
            "items": [
              { "name": "Guinness" },
              { "name": "Sully" },
              { "name": "Ren" }
            ]
          }]
        }
      }
    },
    { "plugin": "./routes/home" },
    {
      "plugin": {
        "register": "good",
        "options": {
          "reporters": {
            "console": [
              {
                "module": "good-squeeze",
                "name": "Squeeze",
                "args": [{
                  "response": "*",
                  "log": "*"
                }]
              },
              { "module": "good-console" },
              "stdout"
            ]
          }
        }
      }
    },
    {
      "plugin": { "register": "./routes/bap" },
      "options": { "routes": { "prefix": "/woot" } }
    }
  ]
}
